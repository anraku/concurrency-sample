package main

import (
	"fmt"
	"sync"
	"time"
)

var wg sync.WaitGroup

func expensiveMethod() error {
	for i := 0; i < 10000000; i++ {
	}
	return nil
}

func main() {
	start := time.Now()
	for i := 0; i < 30; i++ {
		wg.Add(1)
		go func() {
			defer wg.Done()
			expensiveMethod()
		}()
	}
	wg.Wait()
	end := time.Now()
	fmt.Printf("%fç§’\n", (end.Sub(start)).Seconds())
}
